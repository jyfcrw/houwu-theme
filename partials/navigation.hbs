<ul class='nav'>
    {{#foreach navigation}}
        <li class="nav-{{slug}} {{#if current}} nav-current{{/if}}" role="presentation">
            <a href="{{url absolute="true"}}">
                <img src="{{asset_by "images/nav-%s.png" value=slug}}" alt="{{label}}" />
            </a>
        </li>
    {{/foreach}}
</ul>
